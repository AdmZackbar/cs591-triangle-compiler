let
    var a := 2;
    var b := 2;
    proc test1 (in out x: Integer) ~
    begin
        x := x + 1;
        x := x * a;
        putint(x);
    end;
    proc test2 (var x: Integer) ~
    begin
        x := x + 1;
        x := x * b;
        putint(x);
    end
in begin
    putint(a);
    puteol();
    test1(var a);
    puteol();
    putint(a);
    puteol();
    putint(b);
    puteol();
    test2(var b);
    puteol();
    putint(b);
end
