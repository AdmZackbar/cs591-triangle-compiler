! 9.11
let
    enum type colors ~ (red, green, blue, purple, orange, yellow);
    var favorite: colors;
    var primary: array 3 of colors
in begin
    favorite := red;
    primary[0] := red;
    primary[1] := blue;
    primary[2] := green;
    if favorite = blue then
        put('b')
    else
        put('?');
    puteol();
end
