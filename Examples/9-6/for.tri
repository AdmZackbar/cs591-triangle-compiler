begin
    ! For loop will execute until identifier reaches target value
    for i from 0 to 5 do begin
        putint(i);
        puteol();
    end;
    puteol();
    ! For loop will not execute if the target is less than the initial value
    for i from 1 to 0 do begin
        putint(i);
        puteol();
    end;
    puteol();
    ! For loop can use general expression in the header
    for i from 10-2 to 10+2 do begin
        putint(i);
        puteol();
    end;
end
